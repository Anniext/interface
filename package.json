        "@eslint/json": "^0.13.2",
        "@eslint/markdown": "^7.5.0",
        "@tailwindcss/vite": "^4.1.16",
        "@types/matter-js": "^0.20.2",
        "@types/node": "^24.6.0",
        "@vitejs/plugin-vue": "^6.0.1",
